          "datasource": {
            "type": "elasticsearch",
            "uid": "es"
          },
          "metrics": [
            {
              "id": "1",
              "settings": {
                "limit": "500"
              },
              "type": "logs"
            }
          ],
          "query": "_tag : \"fluentbit.logs\"",
          "refId": "A",
          "timeField": "@timestamp"
        }
      ],
      "title": "Fluent Bit Logs",
      "transformations": [
        {
          "id": "filterFieldsByName",
          "options": {
            "include": {
              "names": [
                "@timestamp",
                "level",
                "logger",
                "msg"
              ]
